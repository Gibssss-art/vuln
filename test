local player = game.Players.LocalPlayer
local char = player.Character or player.CharacterAdded:Wait()
local humanoid = char:WaitForChild("Humanoid")

loadstring([[
    function LPH_NO_VIRTUALIZE(f) return f end;
]])();
local version = LRM_ScriptVersion and "v" .. table.concat(LRM_ScriptVersion:split(""), ".") or "Dev Version"
local natui_url = game:HttpGet("https://raw.githubusercontent.com/dy1zn4t/bmF0dWk-/refs/heads/main/ui.lua")
local NatUI = loadstring(natui_url)()
local IsOnMobile = table.find({Enum.Platform.Android, Enum.Platform.IOS}, game:GetService("UserInputService"):GetPlatform())
local WindowSize = IsOnMobile and UDim2.fromOffset(528, 334) or UDim2.fromOffset(580, 350)

local Window = NatUI:CreateWindow({
    Title = "NatHub",
    Icon = "rbxassetid://99764942615873",
    Author = (premium and "Premium" or "Blox Fruit") .. " | " .. version,
    Folder = "NatHub",
    Size = WindowSize,
    LiveSearchDropdown = true,
})


local Tabs = {}
Tabs.UpdateTab = Window:Tab({ Title = "New Update", Icon = "badge-dollar-sign" })
Tabs.MainTab = Window:Tab({ Title = "Main", Icon = "landmark" })
Tabs.FarmTab = Window:Tab({ Title = "Farm", Icon = "badge-dollar-sign" })
Tabs.QuestTab = Window:Tab({ Title = "Quest", Icon = "book-key" })
Tabs.ItemTab = Window:Tab({ Title = "Items", Icon = "box" })
Tabs.FruitTab = Window:Tab({ Title = "Fruit", Icon = "apple" })
Tabs.RaidTab = Window:Tab({ Title = "Raid", Icon = "vegan" })
Tabs.PvpTab = Window:Tab({ Title = "Pvp", Icon = "person-standing" })
Tabs.SeaTab = Window:Tab({ Title = "Sea Event", Icon = "waves" })
Tabs.ShopTab = Window:Tab({ Title = "Shop", Icon = "store" })
Tabs.TeleportTab = Window:Tab({ Title = "Teleport", Icon = "scan-barcode" })
Tabs.VulcanoTab = Window:Tab({ Title = "Vulcano Event", Icon = "mountain-snow" })
Tabs.DragonTab = Window:Tab({ Title = "Dragon Dojo", Icon = "bone" })
Tabs.RaceTab = Window:Tab({ Title = "Race", Icon = "bolt" })
Tabs.EspTab = Window:Tab({ Title = "Esp", Icon = "eye" })
Tabs.SettingTab = Window:Tab({ Title = "Setting", Icon = "cog" })
Tabs.MiscTab = Window:Tab({ Title = "Misc", Icon = "settings-2" })
