            end; l.Init(r, "Icon") function p.SanitizeFilename(v)
                local x = v:match"([^/]+)$" or v;
                x = x:gsub("%.[^%.]+$", "");
                x = x:gsub("[^%w%-_]", "_");
                if #x > 50 then x = x:sub(1, 50); end;
