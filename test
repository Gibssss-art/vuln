            end; function p.Image(v, x, B, C, F, G, H, J)
                C = C or "Temp"; x = p.SanitizeFilename(x);
                local L = r("Frame", { Size = Dim2(0, 0, 0, 0), BackgroundTransparency = 1},
                    {
                        r( "ImageLabel", {
                            Size = Dim2(1, 0, 1, 0),
                            BackgroundTransparency = 1,
